<stall>
  <name>stall-1</name>
  <mode>looped</mode>
  <path>/Aircraft/787/Sounds/stall.wav</path>
  <condition>
    <and>
      <equals>
        <property>/sim/current-view/cockpit</property>
        <value>1</value>
      </equals>
      <greater-than>
        <property>/sim[0]/time/elapsed-sec</property>
        <value>10</value>
      </greater-than>
      <less-than>
        <property>/velocities/airspeed-kt</property>
        <value>150</value>
      </less-than>
      <less-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.015</value>
      </less-than>
      <not>
        <or>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>sim/crashed</property>
        </or>
      </not>
    </and>
  </condition>
  <volume>
    <max>1.0</max>
  </volume>
</stall>

