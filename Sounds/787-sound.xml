	<veeone>
        <name>vee-one</name>
        <mode>once</mode>
        <type>avionics</type>
        <path>Aircraft/787/Sounds/v1.wav</path>
        <condition>
			<or>
			<property>/gear/gear[0]/wow</property>
			<property>/gear/gear[2]/wow</property>
			</or>	
			<greater-than>
				<property>/velocities/airspeed-kt</property>
				<value>140</value>
			</greater-than>
			<less-than>
				<property>/velocities/airspeed-kt</property>
				<value>148</value>
			</less-than>			
     </condition>	
    </veeone>
	
       <rotate>
        <name>rotate</name>
        <mode>once</mode>
        <type>avionics</type>
        <path>Aircraft/787/Sounds/vr.wav</path>
        <condition>
			<or>
			<property>/gear/gear[0]/wow</property>
			<property>/gear/gear[2]/wow</property>
			</or>
			<greater-than>
				<property>/velocities/airspeed-kt</property>
				<value>150</value>
			</greater-than>
			<less-than>
				<property>/velocities/airspeed-kt</property>
				<value>160</value>
			</less-than>			
		</condition>	
    </rotate>	
    
<avionics>
			<name>avionics</name>
			<path>Aircraft/787/Sounds/avionics.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<greater-than>
				<property>systems/electrical/outputs/efis</property>
					<value>0</value>
				</greater-than>
			</condition>
			<position>
				<x>-17</x>
				<y>0.01</y>
				<z>-1.95</z>
			</position>	
			<volume>
				<factor>0.2</factor>
				<max>0.5</max>
			</volume>
			<reference-dist>1.2</reference-dist>
			<max-dist>5</max-dist>
		</avionics>

      <gpws>
        <name>appr_mins</name>
        <mode>once</mode>
        <type>avionics</type>		
        <path>Aircraft/787/Sounds/approaching-minimums.wav</path>
        <condition>
			<less-than>
				<property>/position/altitude-agl-ft</property>
				<value>380</value>
			</less-than>
			<greater-than>
				<property>/position/altitude-agl-ft</property>
				<value>300</value>
			</greater-than>			
			<less-than>
				<property>/velocities/vertical-speed-fps</property>
				<value>0</value>
			</less-than>
		</condition>
          </gpws>
		
		<seatbelt>
			<name>cabinalert</name>
			<path>Aircraft/787/Sounds/cabinalert.wav</path>
			<mode>once</mode>
			<condition>
				<or>
					<property>sim/sound/no-smoking-sign</property>
					<property>sim/sound/seatbelt-sign</property>
					
					<not><property>sim/sound/no-smoking-sign</property></not>
					<not><property>sim/sound/seatbelt-sign</property></not>
				</or>
			</condition>
			<volume>
				<property>sim/current-view/internal</property>
			</volume>
		</seatbelt>

